docker pull --platform=linux ubuntu
docker run --platform=linux -d ubuntu /bin/sh -c "while true; do echo hello world; sleep 1; done"
docker run --platform=linux -d ubuntu /bin/bash -c "while true; do echo hello world; sleep 1; done"
--mo terminal shell trong container đang chạy
docker exec -it ubuntu_bash /bin/bash
--xem các tiến trình đang chạy trong container
docker exec -it ubuntu_bash ps aux


docker run -d microsoft/windowsservercore ping -t 127.0.0.1
docker inspect --format '{{.Os}}' ubuntu